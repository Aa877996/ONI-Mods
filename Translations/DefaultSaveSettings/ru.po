msgctxt "DefaultSaveSettings.OPTIONS.AUTOSAVEINTERVAL.NAME"
msgid "Default Auto Save Interval"
msgstr "Интервал автосохранений"

msgctxt "DefaultSaveSettings.OPTIONS.AUTOSAVEINTERVAL.TOOLTIP"
msgid "Controls the default auto save interval."
msgstr "Управляет интервалом автоматического сохранения."

msgctxt "DefaultSaveSettings.OPTIONS.DISINFECTMINGERMCOUNT.NAME"
msgid "Default Germ Count for Disinfect"
msgstr "Автоматическая дезинфекция"

msgctxt "DefaultSaveSettings.OPTIONS.DISINFECTMINGERMCOUNT.TOOLTIP"
msgid "Controls the default minimum germ count for auto disinfection."
msgstr ""
"Контролирует минимальное количество микробов для автоматической "
"дезинфекции."

msgctxt "DefaultSaveSettings.OPTIONS.ENABLEAUTODISINFECT.NAME"
msgid "Enable Auto Disinfect Default"
msgstr "Включить дезинфекцию"

msgctxt "DefaultSaveSettings.OPTIONS.ENABLEAUTODISINFECT.TOOLTIP"
msgid ""
"If checked, the 'Disinfect At' option will default to true for new games."
msgstr "Если флажок установлен, дезинфекция будет включена для новых игр."

msgctxt "DefaultSaveSettings.OPTIONS.ENABLEPROXIMITY.NAME"
msgid "Enable Proximity Default"
msgstr "Приоритет по близости"

msgctxt "DefaultSaveSettings.OPTIONS.ENABLEPROXIMITY.TOOLTIP"
msgid ""
"If checked, the 'Enable Proximity' option is checked by default for new "
"games."
msgstr ""
"Если этот флажок установлен, опция «Вкл. приоритет по близости» будет "
"включена для новых игр."

msgctxt "DefaultSaveSettings.OPTIONS.Germs.CATEGORY"
msgid "Disease"
msgstr "Микробы"

msgctxt "DefaultSaveSettings.OPTIONS.TIMELAPSERESOLUTION.NAME"
msgid "Default Timelapse Resolution"
msgstr "Разрешение таймлапса"

msgctxt "DefaultSaveSettings.OPTIONS.TIMELAPSERESOLUTION.TOOLTIP"
msgid "Controls the default timelapse resolution."
msgstr "Управляет разрешением таймлапса."
